#include<iostream>
using namespace std;

int main() {

    int x,operator,y;
    cin>>x>>operator>>y;

    if (operator == +) {
        cout<< x + y;
    }
    elif (operator == -) {
        cout<< x - y;
    }
    elif (operator == *) {
        cout<< x * y;
    }
    elif (operator == /) {
        cout<< x / y;
    }
    elif (operator == %) {
        cout<< x % y;
    }
    else {
        cout<<"Invalid Operator";
    }

    return 0;


}